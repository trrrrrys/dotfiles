{
  "gopls": {
    "initialization_options": {
      "staticcheck": true,
      "completeUnimported": true,
      "usePlaceholders": true,
      "completionDocumentation": true,
      "analyses": {
        "fillstruct": true,
        "staticcheck": true
      },
      "matcher": "fuzzy",
      "codelenses": {
        "generate": true,
        "test": true,
        "tidy": true,
        "vendor": false,
        "run_vulncheck": true,
        "upgrade_dependency": true
      },
      "ui.inlayhint.hints": {
        "assignVariableTypes": true,
        "compositeLiteralFields": true,
        "compositeLiteralTypes": true,
        "constantValues": true,
        "functionTypeParameters": true,
        "parameterNames": true,
        "rangeVariableTypes": true
      },
      "hoverKind": "SynopsisDocumentation",
      "linksInHover": false,
      "build.buildFlags": ["-tags=wireinject"],
      "build.standaloneTags": ["ignore", "wireinject", "!wireinject"],
      "build.directoryFilters": ["-**/node_modules", "-**/test", "-**/testdata", "-**/mock_domain", "-**/mock_infra", "-**/mock_usecase"]
    },
    "capabilities": {}
  },
  "golangci-lint-langserver": {
    "initialization_options": {
      "command": [
        "sh", "-c",
        "go tool golangci-lint run --output.json.path stdout || golangci-lint run --output.json.path stdout"
      ]
    }
  },
  "pyls-all": {
    "disabled": true
  },
  "yaml-language-server": {
    "disabled": true
  },
  "efm-langserver": {
    "disabled": false,
    "allowlist": [
      "go",
      "proto",
      "javascript",
      "typescript",
      "javacriptreact",
      "typescriptreact",
      "html",
      "css",
      "json",
      "jsonc",
      "json5",
      "yaml"
    ]
  },
  "vscode-json-language-server": { "disabled": true },
  "typescript-language-server": { "disabled": false }
}
