{
	"uuid": {
		"prefix": ["uuid"],
		"body": [
			"${VIM:system('deno eval --no-lock \"Deno.stdout.writeSync(new TextEncoder().encode(crypto.randomUUID()))\"')}"
		]
	},
	"ulid": {
		"prefix": ["ulid"],
		"body": [
			"${VIM:system('deno eval --no-lock \"import * as mod from \\\"https://deno.land/x/ulid@v0.3.0/mod.ts\\\"; Deno.stdout.writeSync(new TextEncoder().encode(mod.ulid()))\"')}"
		]
	},
	"ulidz": {
		"prefix": ["ulidz"],
		"body": [
			"${VIM:system('deno eval --no-lock \"import * as mod from \\\"https://deno.land/x/ulid@v0.3.0/mod.ts\\\"; Deno.stdout.writeSync(new TextEncoder().encode(mod.ulid().substring(0,10)+\\\"0\\\".repeat(16)))\"')}"
		]
	},
	"Unix Timestamp": {
		"prefix": ["nowunix"],
		"body": [
			"${VIM:strftime(\"%s\")}"
		]
	},
	"Now ISO TimeStamp": {
		"prefix": ["nowiso"],
		"body": [
			"${VIM:strftime(\"%Y-%m-%dT%H:%M:%S%z\")}"
		]
	},
	"---": {
		"prefix": "---",
		"body": "oO0OoO0OoO0Oo"
	}
}
