	"test function": {
		"prefix": ["test"],
		"body": ["func Test${1:Function}(t *testing.T) {", "\t$2", "}"]
	},
	"table test": {
		"prefix": ["tt"],
		"body": [
			"tests := []struct {",
			"\tname string",
			"\tin string",
			"\texpect string",
			"}{",
			"\t{$1},",
			"}",
			"for _, tt := range tests {",
			"\ttt := tt",
			"\tt.Run(tt.name, func(t *testing.T){",
			"\t\tgot, err := f()",
			"\t\tif err != nil {",
			"\t\t\tt.Fatalf(\"error: %+v\", err)",
			"\t\t}",
			"\tif tt.expect != got {",
			"\t\t\tt.Errorf(\"expect: %v, got: %v\", tt.expect, got)",
			"\t}",
			"\t})",
			"}"
		]
	},
	"table test (use reflect)": {
		"prefix": ["ttr"],
		"body": [
			"var tests = []struct {",
			"\tname string",
			"\tin string",
			"\texpect string",
			"}{",
			"\t{$1},",
			"}",
			"for _, tt := range tests {",
			"\ttt := tt",
			"\tt.Run(tt.name, func(t *testing.T){",
			"\t\tgot, err := f()",
			"\t\tif err != nil {",
			"\t\t\tt.Fatalf(\"error: %+v\", err)",
			"\t\t}",
			"\tif !reflect.DeepEqual(tt.expect, got) {",
			"\t\t\tt.Errorf(\"expect: %v, got: %v\", tt.expect, got)",
			"\t}",
			"\t})",
			"}"
		]
	},
	"table test (use gocmp)": {
		"prefix": ["ttcmp"],
		"body": [
			"var tests = []struct {",
			"\tname string",
			"\tin string",
			"\texpect string",
			"\t}{",
			"{$1},",
			"}",
			"for _, tt := range tests {",
			"\ttt := tt",
			"\tt.Run(tt.name, func(t *testing.T){",
			"\t\tgot, err := f()",
			"\t\tif err != nil {",
			"\t\t\tt.Fatalf(\"error: %+v\", err)",
			"\t\t}",
			"\tif !reflect.DeepEqual(tt.expect, got) {",
			"\t\t\tt.Errorf(cmp.Diff(tt.expect, got))",
			"\t}",
			"\t})",
			"}"
		]
	},
	"table test (salon version)": {
		"prefix": ["ttsalon"],
		"body": [
			"type (",
      "\tin struct {",
			"\t\t",
			"\t}",
			"\twant struct {",
			"\t\terr error",
			"\t}",
			")",
			"var tests = []struct {",
			"\tname string",
			"\tin in",
			"\twant want",
			"\texpectErr bool",
			"\tprepare func()",
			"\t}{",
			"{$1},",
			"}",
			"for _, tt := range tests {",
			"\ttt := tt",
			"\tt.Run(tt.name, func(t *testing.T){",
			"\t\ttt.prepare()",
			"\t\tgot, err := f()",
			"\t\tif err != nil {",
			"\t\t\tt.Fatalf(\"error: %+v\", err)",
			"\t\t}",
			"\tif !cmp.Equal(tt.want, got) {",
			"\t\t\tt.Errorf(cmp.Diff(tt.want, got))",
			"\t}",
			"\t})",
			"}"
		]
	},
